generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}


model Transaction {
  id        Int              @id @default(autoincrement())
  amount    Float
  category  String
  note      String?
  type      TransactionType
  date      DateTime         @default(now())
  createdAt DateTime         @default(now())
  updatedAt DateTime         @updatedAt
}
